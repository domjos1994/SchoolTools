<resources>
    <declare-styleable name="LabelledSeekBar">
        <attr name="editableLabel" format="boolean"/>
        <attr name="max" format="integer"/>
        <attr name="current" format="float"/>
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="title" format="string" />
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="SwipeRefreshDeleteList">
        <attr name="itemIcon" format="integer"/>
    </declare-styleable>

    <declare-styleable name="WidgetCalendar">
        <attr name="toolBarBackground" format="integer"/>
        <attr name="monthViewBackground" format="integer"/>
        <attr name="dayViewBackground" format="integer"/>
        <attr name="focusBackground" format="integer"/>
        <attr name="groupLabelWidth" format="dimension"/>
    </declare-styleable>
</resources>